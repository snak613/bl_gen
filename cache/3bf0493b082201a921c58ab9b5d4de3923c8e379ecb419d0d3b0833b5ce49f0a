        function onSubmit(token) {
            // If we don't use setTimeout reportValidity doesn't work properly
            setTimeout(() => {
                const formEl = document.querySelector('form[data-recaptcha-form]');

                if(formEl.reportValidity()){
                    formEl.submit();
                } else {
                    grecaptcha.reset();
                }
            }, 0);

        }
